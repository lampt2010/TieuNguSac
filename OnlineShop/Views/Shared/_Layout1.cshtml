<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />

    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://fonts.googleapis.com/css?family=Open+Sans" rel="stylesheet">
    <link rel="stylesheet" href="/assets/css/bootstrap.min.css">
    <link rel="stylesheet" href="/assets/css/style.css">
    <link rel="stylesheet" href="/assets/css/font-awesome.min.css">
    <link rel="stylesheet" href="/assets/css/swiper.min.css">
    <script type="text/javascript" src="/assets/js/jquery-1.11.3.min.js"></script>
    <title>@ViewBag.Title</title>
</head>
<body>
    
    @{ Html.RenderAction("TopMenu1", "Home");}

    @{ Html.RenderAction("Slide1", "Home");}
  



        @RenderBody()
    
    @{ Html.RenderAction("Footer1", "Home");}








    <!-- End footer -->
    <script src="/assets/js/swiper/swiper.min.js"></script>
    <script src="/assets/js/responsive_top_menu.js"></script>
    <!-- Main menu -->
    <script src="/assets/js/responsive_main_menu.js"></script>
    <!-- Main js -->
    <script type="text/javascript" src="/assets/js/tieungusac.js"></script>
    <script>
	    var wrapper = document.querySelector(".top_menu");
	    var nav = priorityNav.init({
	        mainNavWrapper: ".top_menu", // mainnav wrapper selector (must be direct parent from mainNav)
	        mainNav: ".top_nav_menu", // mainnav selector. (must be inline-block)
	        navDropdownLabel: '...',
	        navDropdownClassName: "nav__dropdown", // class used for the dropdown.
	        navDropdownToggleClassName: "nav__dropdown-toggle", // class used for the dropdown toggle.
	    });

    </script>
    <script type="text/javascript">
		(function(){
			/****************************************************************
			*****************************Product slide***********************
			***************************************************************** */
			"use strict";
			 var swiper = new Swiper('.tre_thai', {
				nextButton: '.product_bt_next',
        		prevButton: '.product_bt_prev',
		        slidesPerView: 4,
		        paginationClickable: true,
		        loop: true,
		        keyboardControl: true,
		        speed: 600,
		        breakpoints: {
				    480: {
				      slidesPerView: 1,
				    },
				    // when window width is <= 640px
				    640: {
				      slidesPerView: 2,
				    },
				    // when window width is <= 991px
				    991: {
				      slidesPerView: 3,
				    }
				}
			});
		})(jQuery);
    </script>
    @RenderSection("jsFooter", required: false)
</body>
</html>
